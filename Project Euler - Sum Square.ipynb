    "def difference(num):\n",
    "    sq_sum=0\n",
    "    for i in range(num+1):\n",
    "        sq_sum+=i**2\n",
    "    sum_sq=sum(range(num+1))**2\n",
    "    return sum_sq-sq_sum"
   ]
